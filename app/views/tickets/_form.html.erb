<%= form_for @ticket do |f| %>
  <div class="form-group" style="gap: 20px;">
    <%= f.label "Ticket Name" %>
    <%= f.text_field :ticket_name %>
    <br>
    <br>
    <%= f.label "Issue" %>
    <%= f.text_field :issue %>
    <br>
    <br>
    <%= f.label "Documentation" %>
    <%= f.text_field :documentation %>
    <br>
    <br>
    <%= f.label "developer" %>
    <%= f.text_field :developer %>
    <br>
    <br>
    <%= f.label "story points" %>
    <%= f.text_field :story_points %>
    <br>
    <br>
    <%= f.label "accurate" %>
    <%= f.text_field :accurate %>
    <br>
    <br>
    <%= f.label "commit" %>
    <%= f.text_field :commit %>
    <br>
    <br>
    
  </div>
  <br>
  <br>
  <button class="btn"><%=f.submit%></button>
  <%= link_to "Return Home", root_path, class: "new-ticket-btn" %>
<% end %>